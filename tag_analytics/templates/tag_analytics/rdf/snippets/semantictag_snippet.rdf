<stodap:SemanticTag rdf:about="http://{{ request.get_host }}{% url 'semantictag_rdf' semantictag.id %}">

    <dct:description>{{ semantictag.description }}</dct:description>
    <rdfs:label>{{ semantictag.name }}</rdfs:label>
    <dct:identifier>{{ semantictag.name }}</dct:identifier>
    <dct:title>{{ semantictag.name }}</dct:title>
    <muto:tagMeaning rdf:resource="{{ semantictag.uri }}" />
<!-- Local Tags -->
{% for tag in semantictag.tags.all %}
    <stodap:hasTag rdf:resource="http://{{ request.get_host }}{% url 'tag_rdf' tag.id %}"/>{% endfor %}
<!-- Semantic Groups -->
{% for sem_group in semantictag.globalgroups.all %}
    <stodap:sGrouppedBy rdf:resource="http://{{ request.get_host }}{% url 'semanticgroup_rdf' sem_group.id %}"/>{% endfor %}
<!-- Narrower -->
{% for n in semantictag.narrower.all %}
    <skos:narrower rdf:resource="http://{{ request.get_host }}{% url 'semantictag_rdf' n.id %}"/>{% endfor%}
<!-- <Broader -->
{% for b in semantictag.broader.all %}
    <skos:broader rdf:resource="http://{{ request.get_host }}{% url 'semantictag_rdf' b.id %}"/>{% endfor%}
<!-- Related -->
{% for r in semantictag.related.all %}
    <skos:related rdf:resource="http://{{ request.get_host }}{% url 'semantictag_rdf' r.id %}"/>{% endfor%}
</stodap:SemanticTag>